package sp1;

import java.sql.Connection;

public class product_ok {
	Connection con = null;
	private void dbcon() {
		try {
			this.con = dbconfig.info(); //dconfig 안의 info를 가지고오자
		}
		catch(Exception e) {
			System.out.println("DB 접속오류!!");
		}
	}
	
	protected int modify_sql() {
		try {}
		catch(Exception e) {
			System.out.println("SQL 문법 노류발생");
		}
		
		return 0;
	}
}
