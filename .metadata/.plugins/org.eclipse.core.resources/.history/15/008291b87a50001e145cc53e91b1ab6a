import java.net.ServerSocket;
import java.net.Socket;

public class server {

	public static void main(String[] args) {
		int port = 8009; 
		ServerSocket ss = null; 
		Socket sc = null;  
		try {
	
			ss = new ServerSocket(port); 
			System.out.println("Server Open!!");
			sc = ss.accept(); 
		}
		catch(Exception e) {
			System.out.println("Server Open Error!!");
		}

	}

}
